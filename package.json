{
  "type": "module",
  "name": "@chrisnski/webshorts",
  "version": "1.0.14",
  "description": "A lightweight react shortcut system with global and page-level support, built for developer sanity of quickly managing shortcut systems.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE",
    "webshorts.config.js"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "echo \\\"No tests currently available\\\" && exit 0",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "webshorts": "./bin/init.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisNSki/webshorts.git"
  },
  "keywords": [
    "shortcuts",
    "keyboard",
    "hotkeys",
    "react",
    "context",
    "productivity",
    "webshorts"
  ],
  "author": "Chris Narowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChrisNSki/webshorts/issues"
  },
  "homepage": "https://webshorts.dev",
  "devDependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@vitejs/plugin-react": "^4.6.0",
    "sonner": "^2.0.6",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "sonner": "^2.0.6"
  },
  "dependencies": {}
}
